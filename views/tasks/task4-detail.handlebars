<!--contain-->
<div class="container">
  <h1 class="text-center">{{data.name}} Characteristics</h1>
</div>
<br /><br />
<div class="container well span6">
  <div class="row">
    <div class="col-md-4">
      <img src="../{{data.imagePath}}" class="img-circle w-75" />
    </div>

    <div class="col-md-8">
      <h3>{{data.name}}</h3>
      <h4>Strengths: <small>{{data.strengths}}</small></h4>
      <h4>Weaknesses: <small>{{data.weaknesses}}</small></h4>
      <h4>Likes: <small>{{data.likes}}</small></h4>
      <h4>Dislikes: <small>{{data.dislikes}}</small></h4>
      <h4><small>{{data.description}}</small></h4>
    </div>
  </div>
</div>
<!--/contain-->